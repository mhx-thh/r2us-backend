{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
        "no-use-before-define": ["error", { "variables": false }],
        "no-multi-str": 0,
        "func-names": ["error", "never"],
        "prefer-destructuring": ["error", {"object": true, "array": false}]
    }
}
